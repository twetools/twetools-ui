  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "twetools",
    "typescript"
  ],
  "author": "Troy Edwards <troy@twetools.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twetools-admin/twetools-ui.git"
